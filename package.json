{
  "name": "properties-to-prototype",
  "version": "2.0.0",
  "description": "Moves properties out of classes to prototypes, ensures not minified class name.",
  "author": "Ferdinand Prantl <prantlf@gmail.com>",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/properties-to-prototype/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/properties-to-prototype#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/properties-to-prototype.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/properties-to-prototype/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "biome lint *.js lib test",
    "check": "teru-esm test/*.js",
    "cover": "c8 teru-esm test/*.js && c8 report -r text -r lcov",
    "test": "biome lint *.js lib test && c8 teru-esm test/*.js && c8 report -r text -r lcov"
  },
  "c8": {
    "reporter": []
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "astring": "^1.9.0",
    "c8": "^10.1.3",
    "meriyah": "^7.0.0",
    "rollup": "^4.53.3",
    "tehanu": "^1.0.1",
    "tehanu-repo-coco": "^1.0.1",
    "tehanu-teru": "^1.0.1"
  },
  "keywords": [
    "backbone",
    "marionette",
    "class",
    "syntax",
    "class-syntax",
    "properties",
    "prototype"
  ]
}